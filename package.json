{
  "name": "Teamlead-TodoList",
  "version": "0.0.1",
  "private": true,
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "type": "module",
  "scripts": {
    "build-jsx-browser": "parcel build --log-level 1 --global __root_component --out-dir views views/*.jsx",
    "build-jsx-node": "parcel build --log-level 1 --no-minify --target node --out-dir views/node views/*.jsx",
    "build": "concurrently -i 'npm:build-*'",
    "start": "node --es-module-specifier-resolution=node app.js",
    "watch-server": "nodemon --es-module-specifier-resolution=node -e js app.js",
    "watch-jsx-browser": "parcel watch --log-level 1 --no-hmr --global __root_component --out-dir views views/*.jsx",
    "watch-jsx-node": "parcel watch --log-level 1 --target node --out-dir views/node views/*.jsx",
    "watch-jsx": "concurrently 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "watch": "concurrently -i 'npm:watch-server' 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "lint": "eslint app.js routes"
  },
  "dependencies": {
    "@atlaskit/avatar": "^21.15.2",
    "@atlaskit/button": "20.2.1",
    "@atlaskit/checkbox": "14.0.2",
    "@atlaskit/form": "10.5.4",
    "@atlaskit/logo": "^14.3.1",
    "@atlaskit/lozenge": "^11.10.2",
    "@atlaskit/page": "12.6.0",
    "@atlaskit/pagination": "^14.9.1",
    "@atlaskit/section-message": "6.6.5",
    "@atlaskit/select": "17.19.1",
    "@atlaskit/side-navigation": "^3.5.2",
    "@atlaskit/textfield": "^6.5.2",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@types/react": "^18.3.10",
    "atlassian-connect-express": "11.4.1",
    "axios": "1.7.7",
    "body-parser": "1.20.3",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "errorhandler": "1.5.1",
    "express": "4.21.0",
    "express-hbs": "^2.5.0",
    "helmet": "8.0.0",
    "js-yaml": "^4.1.0",
    "mobx": "^6.13.3",
    "mobx-react-lite": "^4.0.7",
    "morgan": "1.10.0",
    "nocache": "4.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sequelize": "^6.37.3",
    "styled-components": "6.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.37.0",
    "longjohn": "^0.2.12",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.7",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-externals": "^0.5.2",
    "sqlite3": "^5.1.7"
  }
}
